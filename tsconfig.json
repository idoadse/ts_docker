{
    "compilerOptions": {
        "module": "commonjs",
        "noImplicitAny": true,
        "sourceMap": true,
        "target": "es5",
        "experimentalDecorators": true,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "lib": [ "es2015" ],
        "outDir": "dist"
    },
    "include": [
      "./src/*"
    ],
    "exclude": [
      "node_modules"
    ]
  }